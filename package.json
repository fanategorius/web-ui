    "moment": "2.29.4",
    "moment-duration-format": "2.3.2",
    "prop-types": "15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.3.0",
    "react-router-navigation-prompt": "1.9.1",
    "redux": "^4.1.2",
    "redux-saga": "1.1.3",
    "semver": "7.3.5"
  },
  "resolutions": {
    "immer": ">=9.0.6",
    "underscore": "1.13.1"
  },
  "scripts": {
    "preinstall": "test -e scripts/yarn-preinstall.sh && scripts/yarn-preinstall.sh || :",
    "clean": "make distclean",
    "dev": "NODE_ENV=development V=1 webpack --progress --config config/webpack.config.dev.default.js",
    "start": "NODE_ENV=development BABEL_ENV=development node scripts/start.js",
    "prebuild": "yarn run test",
    "build": "NODE_ENV=production V=1 webpack --config config/webpack.config.prod.js",
    "pretest": "node scripts/pretest.js",
    "test": "jest --no-watchman",
    "test:watch": "yarn run pretest && jest --watch --no-watchman",
    "eslint": "eslint src config scripts babel-plugin",
    "eslint:fix": "eslint src config scripts babel-plugin --fix",
    "intl:dummy": "babel-node --presets @babel/env,@babel/flow scripts/intl/dummy-locale.js",
    "intl:extract": "scripts/intl/intl-extract.sh",
    "intl:push": "scripts/intl/intl-zanata-push.sh",
    "intl:pull": "scripts/intl/intl-zanata-pull.sh",
    "intl:apply": "scripts/intl/intl-apply.sh",
    "intl:report": "babel-node --presets @babel/env,@babel/flow scripts/intl/report.js",
    "intl:sync": "babel-node --presets @babel/env,@babel/flow scripts/intl/sync-messages.js",
    "intl:check-use": "scripts/intl/check-message-use.sh"
  },
  "jest": {
    "setupFiles": [],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/babel-plugin/**/*.test.{js,jsx,ts,tsx}"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "_/ovirtapi": "<rootDir>/src/mock/ovirtapi.mock.js",
      "_/(.*)$": "<rootDir>/src/$1"
    }
  },
  "eslintConfig": {
    "extends": "./config/eslint.cjs"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie > 0",
    "not dead"
  ]
}
